SET GRAMCHK-S-BOUNDARY3  = (Pron Interr) OR (Pron Rel) OR ("muhto") OR ("de" CC) OR MO OR ("\;") OR (":") OR ("-") OR ("–") OR CS ;
	# not OR PUNCT-LEFT-RIGHT

SET BOC = S-BOUNDARY OR BOS OR GRAMCHK-S-BOUNDARY ;
SET BOC2 = S-BOUNDARY2 OR BOS OR GRAMCHK-S-BOUNDARY2 ;
SET BOC-PUNCT = BOC - ("-") - ("–") ; 
SET EOC = S-BOUNDARY OR EOS OR GRAMCHK-S-BOUNDARY;
