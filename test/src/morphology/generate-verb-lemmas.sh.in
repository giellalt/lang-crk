#Script adapted to Cree

    egrep -v '^\!' ../../../src/morphology/stems/verbs.lexc |cut -d ' ' -f1,2 | grep ' IIC' | tr '+:' ' ' | cut -d ' ' -f1 > iiverblemmas
    egrep -v '^\!' ../../../src/morphology/stems/verbs.lexc |cut -d ' ' -f1,2 | grep ' TIC' | tr '+:' ' ' | cut -d ' ' -f1 > tiverblemmas
    egrep -v '^\!' ../../../src/morphology/stems/verbs.lexc |cut -d ' ' -f1,2 | grep ' AIC' | tr '+:' ' ' | cut -d ' ' -f1 > aiverblemmas
    egrep -v '^\!' ../../../src/morphology/stems/verbs.lexc |cut -d ' ' -f1,2 | grep ' TAC' | tr '+:' ' ' | cut -d ' ' -f1 > taverblemmas
    egrep -v '^\!' ../../../src/morphology/stems/verbs.lexc |cut -d ' ' -f1,2 | grep ' IIPLC' | tr '+:' ' ' | cut -d ' ' -f1 > iiplverblemmas
    egrep -v '^\!' ../../../src/morphology/stems/verbs.lexc |cut -d ' ' -f1,2 | grep ' TIPLC' | tr '+:' ' ' | cut -d ' ' -f1 > tiplverblemmas
    egrep -v '^\!' ../../../src/morphology/stems/verbs.lexc |cut -d ' ' -f1,2 | grep ' AIPLC' | tr '+:' ' ' | cut -d ' ' -f1 > aiplverblemmas
    egrep -v '^\!' ../../../src/morphology/stems/verbs.lexc |cut -d ' ' -f1,2 | grep ' TAPLC' | tr '+:' ' ' | cut -d ' ' -f1 > taplverblemmas
    cat iiverblemmas |sed 's/$/+V+II+Ind+Prs+3Sg/' |$LOOKUP $GTHOME/langs/crk/src/generator-gt-norm.xfst |cut -f2 > verbgenlemmas
    cat tiverblemmas |sed 's/$/+V+TI+Ind+Prs+3Sg/'|$LOOKUP $GTHOME/langs/crk/src/generator-gt-norm.xfst |cut -f2 >> verbgenlemmas
    cat aiverblemmas |sed 's/$/+V+AI+Ind+Prs+3Sg/'|$LOOKUP $GTHOME/langs/crk/src/generator-gt-norm.xfst |cut -f2 >> verbgenlemmas
    cat taverblemmas |sed 's/$/+V+TA+Ind+Prs+3Sg+3SgO/'|$LOOKUP $GTHOME/langs/crk/src/generator-gt-norm.xfst |cut -f2 >> verbgenlemmas
    cat iiplverblemmas |sed 's/$/+V+II+Ind+Prs+3Pl/' |$LOOKUP $GTHOME/langs/crk/src/generator-gt-norm.xfst |cut -f2 >> verbgenlemmas
    cat tiplverblemmas |sed 's/$/+V+TI+Ind+Prs+3Pl/'|$LOOKUP $GTHOME/langs/crk/src/generator-gt-norm.xfst |cut -f2 >> verbgenlemmas
    cat aiplverblemmas |sed 's/$/+V+AI+Ind+Prs+3Pl/'|$LOOKUP $GTHOME/langs/crk/src/generator-gt-norm.xfst |cut -f2 >> verbgenlemmas
    cat taplverblemmas |sed 's/$/+V+TA+Ind+Prs+3Pl+3SgO/'|$LOOKUP $GTHOME/langs/crk/src/generator-gt-norm.xfst |cut -f2 >> verbgenlemmas
    
    cat *verblemmas | sort -u > allverblemmas
    cat verbgenlemmas | grep '[a-z]' | sort -u > sortverbgenlemmas
    comm -23 allverblemmas sortverbgenlemmas > missingverbs.txt
        rm *verblemmas *genlemmas


    open missingverbs.txt

    
    
