    egrep -v '^\!' ../../../src/morphology/stems/nouns.lexc |cut -d ' ' -f1,2 |grep ' IN' |egrep '(^\-|PL)' |  tr '+:' ' ' | cut -d ' ' -f1 > depINnounlemmas
