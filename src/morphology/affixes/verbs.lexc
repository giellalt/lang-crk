!! !!! Transitive inanimate (TA)
!      ===========================
!      ===========================

!! The VTA verbs split in different orders.
!! There are 5 stem classes in Wolv, 1-5 (VTA-1 through VTA-5)

!!! STEM CLASS

LEXICON TACONJ_1 !!= @CODE@ Regular verbs
+V+TA: VTACONJ_WICI ;

LEXICON TACONJ_2  !!= * @CODE@ -Vw stem-ending verbs; where + i suf deletes w and i
+V+TA: VTACONJvw_WICI ;

LEXICON TACONJ_2-PL  !!= * @CODE@ -Vw stem-ending verbs; when deletes w and i plural only forms
+V+TA: VTACONJvw_PL_WICI ;

LEXICON TACONJ_3  !!= * @CODE@ -Cw stem-ending verbs; where w and i fuse to o
+V+TA: VTACONJcw_WICI;

LEXICON TACONJ_4  !!= * @CODE@ -t stem-ending verbs; where t becomes s in some cases
+V+TA: VTACONJt_WICI ;

LEXICON TACONJ_5 !!= * @CODE@ verbs that take a -i in 2sg imm. imp. due to moraic constraints
+V+TA: VTACONJi_WICI ;

LEXICON TACONJ_5-TS !!= * @CODE@  verbs that take a -i in 2sg imm. imp. due to moraic constraints, but also have stem final t becoming s in some cases.
+V+TA: VTACONJti_WICI ;

!!! WICI

!!!! -t final verbs (refers to the 3 SG form ending)

LEXICON VTACONJ_WICI !!= @CODE@ Regular verbs
@U.wici.NULL@ VTA_ORDER ;

LEXICON VTACONJvw_WICI  !!= * @CODE@ -Vw stem-ending verbs; where + i suf deletes w and i
@U.wici.NULL@ VTAvw_ORDER ;


LEXICON VTACONJvw_PL_WICI  !!= * @CODE@ -Vw stem-ending verbs; when deletes w and i plural only forms 
@U.wici.NULL@ VTAvw_PL_ORDER ;


LEXICON VTACONJcw_WICI  !!= * @CODE@ -Cw stem-ending verbs; where w and i fuse to o 
@U.wici.NULL@ VTAcw_ORDER ;

LEXICON VTACONJt_WICI  !!= * @CODE@ -t stem-ending verbs; where t becomes s in some cases 
@U.wici.NULL@ VTAt_ORDER ;


LEXICON VTACONJi_WICI  !!= * @CODE@ verbs that take a -i in 2sg imm. imp. due to moraic constraints 
@U.wici.NULL@ VTAi_ORDER ;


LEXICON VTACONJti_WICI  !!= * @CODE@  verbs that take a -i in 2sg imm. imp. due to moraic constraints, but also have stem final t becoming s in some cases 
@U.wici.NULL@ VTAti_ORDER ;


