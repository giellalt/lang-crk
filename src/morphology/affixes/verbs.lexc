!! The Plains Cree verbs are divided in four groups:

!! # AI: Animate intransitive 
!! # II: Inanimate intransitive
!! # TA: Transitive animate
!! # TI: Transitive inanimate



!! !!!Prefixes


LEXICON VerbPrefixes  !!= @CODE@ divides the lexicon into four modes: independent, conjunctive, imperative and future conditional

@U.order.indep@ INDEPENDENT ;         !!= * @CODE@
@U.order.cnj@   CONJUNCT ;            !!= * @CODE@ 
               IMPERATIVE ;          !!= * @CODE@
               FUTURE_CONDITIONAL ;  !!= * @CODE@


LEXICON INDEPENDENT !!= @CODE@ gives flags and prefixes for personprefix
@U.person.NI@:@U.person.NI@nit2 IND_TENSE ;
@U.person.KI@:@U.person.KI@kit2 IND_TENSE ;
@U.person.NULL@                 IND_TENSE ;
! < 0:n 0:i  "@U.person.NI@" 0:"t2"                       > IND_TENSE ; !
! < 0:k 0:i  "@U.person.KI@" 0:"t2"                       > IND_TENSE ; !
! <          "@U.person.NULL@" > IND_TENSE ; !


LEXICON IND_TENSE !!= @CODE@ gives flags and prefixes for tense 
@U.tense.Prs@                                                            PREVERBS ;
@U.tense.Prt@@P.joiner.NULL@:@U.tense.Prt@@P.joiner.NULL@kî^EGLOT       PREVERBS ;
@U.tense.Prt@@P.joiner.hyphen@:@U.tense.Prt@@P.joiner.hyphen@kî-        PREVERBS ;
@U.tense.FutDef@@P.joiner.NULL@:@U.tense.FutDef@@P.joiner.NULL@ka^EGLOT PREVERBS ;
@U.tense.FutDef@@P.joiner.hyphen@:@U.tense.FutDef@@P.joiner.hyphen@ka-  PREVERBS ;
@U.tense.FutInt@@P.joiner.NULL@:@U.tense.FutInt@@P.joiner.NULL@wî^EGLOT PREVERBS ;
@U.tense.FutInt@@P.joiner.hyphen@:@U.tense.FutInt@@P.joiner.hyphen@wî-  PREVERBS ;
! <                   "@U.tense.Prs@"                        > PREVERBS ; !
! < 0:k 0:î 0:^EGLOT "@U.tense.Prt@"    "@P.joiner.NULL@"   > PREVERBS ; ! 
! < 0:k 0:î 0:-      "@U.tense.Prt@"    "@P.joiner.hyphen@" > PREVERBS ; !
! < 0:k 0:a 0:^EGLOT "@U.tense.FutDef@" "@P.joiner.NULL@"   > PREVERBS ; !
! < 0:k 0:a 0:-      "@U.tense.FutDef@" "@P.joiner.hyphen@" > PREVERBS ; !
! < 0:w 0:î 0:^EGLOT "@U.tense.FutInt@" "@P.joiner.NULL@"   > PREVERBS ; ! N.B. - move to a next lexicon
! < 0:w 0:î 0:-      "@U.tense.FutInt@" "@P.joiner.hyphen@" > PREVERBS ; !


LEXICON FUTURE_CONDITIONAL !!= @CODE@ gives flags for future conditional (no prefix)
@U.order.FutCon@ PREVERBS ;


LEXICON CONJUNCT !!= @CODE@ gives flag for conjunctive and prefix ê-, kâ-, kâki-
@P.joiner.hyphen@PV/e+:@P.joiner.hyphen@ê- CNJ_TENSE ;
@P.joiner.NULL@PV/e+:@P.joiner.NULL@ê^EGLOT CNJ_TENSE ;
@P.joiner.hyphen@PV/ka+:@P.joiner.hyphen@ka- CNJ_TENSE ;
@P.joiner.NULL@PV/ka+:@P.joiner.NULL@ka^EGLOT CNJ_TENSE ;
@P.joiner.hyphen@PV/kaa+:@P.joiner.hyphen@kâ- CNJ_TENSE ;
@P.joiner.NULL@PV/kaa+:@P.joiner.NULL@kâ^EGLOT CNJ_TENSE ;
@P.joiner.hyphen@PV/kaa_ki+:@P.joiner.hyphen@kâ-ki- CNJ_TENSE ;
@P.joiner.NULL@PV/kaa_ki+:@P.joiner.NULL@kâki^EGLOT CNJ_TENSE ;

@P.cnj.CC@ CNJ_TENSE ; ! Option to allow for the occasional omission of the grammatical conjunct prefix


LEXICON CNJ_TENSE   !!= @CODE@ gives prefixes and flags for tense in conjunctive
@U.tense.Prs@                                                                          PREVERBS ;
@U.tense.Prt@@U.joiner.hyphen@@C.cnj@:@U.tense.Prt@@U.joiner.hyphen@@C.cnj@kî-        PREVERBS ;
@U.tense.Prt@@U.joiner.NULL@@C.cnj@:@U.tense.Prt@@U.joiner.NULL@@C.cnj@kî^EGLOT       PREVERBS ;
@U.tense.FutInt@@U.joiner.hyphen@@C.cnj@:@U.tense.FutInt@@U.joiner.hyphen@@C.cnj@wî-  PREVERBS ;
@U.tense.FutInt@@U.joiner.NULL@@C.cnj@:@U.tense.FutInt@@U.joiner.NULL@@C.cnj@wî^EGLOT PREVERBS ;
! <                   "@U.tense.Prs@"                        > PREVERBS ; ! Present
! < 0:k 0:î 0:-      "@U.tense.Prt@" "@U.joiner.hyphen@" "@C.cnj@"    > PREVERBS ; ! Preterite, 
! < 0:k 0:î 0:^EGLOT "@U.tense.Prt@" "@U.joiner.NULL@" "@C.cnj@"      > PREVERBS ; ! Preterite,
! < 0:w 0:î 0:-      "@U.tense.FutInt@" "@U.joiner.hyphen@" "@C.cnj@" > PREVERBS ; ! Future Intentional
! < 0:w 0:î 0:^EGLOT "@U.tense.FutInt@" "@U.joiner.NULL@" "@C.cnj@"   > PREVERBS ; ! Future Intentional


LEXICON IMPERATIVE   !!= @CODE@ gives flag for imperative (no prefixes)
@U.order.imp@  PREVERBS ;

