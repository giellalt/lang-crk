!! The file


!! !! The prefixes

LEXICON Nouns 

AN ; ! Animate nouns
IN ; ! Inanimate nouns
INALIENABLE ;
ANSTEM_kw ; ! Animate nouns whose stems end in kw
IRREGAN ; !mistatim, atim


LEXICON AN
!! @LEXNAME@ redirects to ANSTEMS
!! At the moment it is just a copy of INSTEMS, 
!! pretending animates and inanimates are identical
!! Flag diacritics direct to possessive declension later on

@U.noun.abs@ ANSTEMLIST ; 

< 0:n 0:i "@U.noun.1sg@"  0:"t2" > ANSTEMS ; ! 1
< 0:k 0:i "@U.noun.2sg@"  0:"t2" > ANSTEMS ; ! 2
< 0:o     "@U.noun.3sg@"  0:"t2" > ANSTEMS ; ! 3
< 0:o     "@U.noun.3isg@" 0:"t2" > ANSTEMS ; ! 3-singular-obviative
< 0:n 0:i "@U.noun.1pl@"  0:"t2" > ANSTEMS ; ! exclusive Pl
< 0:k 0:i "@U.noun.12pl@" 0:"t2" > ANSTEMS ; ! inclusive Pl
< 0:k 0:i "@U.noun.2pl@"  0:"t2" > ANSTEMS ; ! 2pl 
< 0:o     "@U.noun.3pl@"  0:"t2" > ANSTEMS ; ! 3pl
< 0:o     "@U.noun.3ipl@" 0:"t2" > ANSTEMS ; ! 3-plural-obviate


LEXICON IRREGAN

@U.noun.abs@ IRREGANSTEMSLIST ; 


< 0:n 0:i "@U.noun.1sg@" >  IRREGANSTEMS ; ! 1
< 0:k 0:i "@U.noun.2sg@" >  IRREGANSTEMS ; ! 2
< 0:o     "@U.noun.3sg@" >  IRREGANSTEMS ; ! 3
< 0:o     "@U.noun.3isg@" > IRREGANSTEMS ; ! 3-singular-obviative
< 0:n 0:i "@U.noun.1pl@" >  IRREGANSTEMS ; ! exclusive Pl
< 0:k 0:i "@U.noun.12pl@" > IRREGANSTEMS ; ! inclusive Pl
< 0:k 0:i "@U.noun.2pl@" >  IRREGANSTEMS ; ! 2pl 
< 0:o     "@U.noun.3pl@" >  IRREGANSTEMS ; ! 3pl
< 0:o     "@U.noun.3ipl@" > IRREGANSTEMS ; ! 3-plural-obviate

LEXICON IN
!! @LEXNAME@ redirects to INSTEMS in two ways:
!! Without prefixes, forming the absolutive (non-Px) declension

@U.noun.abs@  INSTEMLIST ; 

!! With prefixes, forming the possessive declension
< 0:n 0:i "@U.noun.1sg@"  0:"t2"  > INSTEMS ; !
< 0:k 0:i "@U.noun.2sg@"  0:"t2"  > INSTEMS ; !
< 0:o     "@U.noun.3sg@"  0:"t2"  > INSTEMS ; !
< 0:o     "@U.noun.3isg@" 0:"t2"  > INSTEMS ;    ! 3-singular-obviative
< 0:n 0:i "@U.noun.1pl@"  0:"t2"  > INSTEMS ; ! exclusive Pl
< 0:k 0:i "@U.noun.12pl@" 0:"t2"  > INSTEMS ; ! inclusive Pl
< 0:k 0:i "@U.noun.2pl@"  0:"t2"  > INSTEMS ; !
< 0:o     "@U.noun.3pl@"  0:"t2"  > INSTEMS ; !
< 0:o     "@U.noun.3ipl@" 0:"t2"  > INSTEMS ;       ! 3-plural-obviate



LEXICON INALIENABLE
!! @LEXNAME@ redirects to INALIENABLESTEMS

< "@U.noun.abs@" >  INALIENABLESTEMS ; 

! I comment out this, as it does not match the yaml file for maniway
! < 0:n 0:i "@U.noun.1sg@"  > INALIENABLESTEMS ; ! 1
! < 0:k 0:i "@U.noun.2sg@"  > INALIENABLESTEMS ; ! 2
! < 0:o     "@U.noun.3sg@"  > INALIENABLESTEMS ;     ! 3
! < 0:m 0:i "@U.noun.3isg@" > INALIENABLESTEMS ;     ! 3indef
! < 0:n 0:i "@U.noun.1pl@"  > INALIENABLESTEMS ;   ! exclusive Pl
! < 0:k 0:i "@U.noun.12pl@" > INALIENABLESTEMS ; ! inclusive Pl
! < 0:k 0:i "@U.noun.2pl@"  > INALIENABLESTEMS ;  ! 2pl 
! < 0:o     "@U.noun.3pl@"  > INALIENABLESTEMS;       ! 3pl note: is prefix o- correct?

< 0:n 0:i "@U.noun.1sg@"  > INALIENABLESTEMS ; ! 1
< 0:k 0:a "@U.noun.2sg@"  > INALIENABLESTEMS ; ! 2
< 0:o 0:t 0:a    "@U.noun.3sg@"  > INALIENABLESTEMS ;     ! 3
< 0:m 0:a "@U.noun.3isg@" > INALIENABLESTEMS ;     ! 3indef
< 0:n 0:i "@U.noun.1pl@"  > INALIENABLESTEMS ;   ! exclusive Pl
< 0:k 0:i "@U.noun.12pl@" > INALIENABLESTEMS ; ! inclusive Pl
< 0:k 0:a "@U.noun.2pl@"  > INALIENABLESTEMS ;  ! 2pl 
< 0:n 0:a  "@U.noun.3pl@"  > INALIENABLESTEMS;       ! 3pl note: is prefix o- correct?


LEXICON ANSTEM_kw
!! @LEXNAME@ is for a set of nouns that take an additional suffix in the possessive paradigm
< "@U.noun.abs@" >  ANSTEMS_kw ;

< 0:n 0:i "@U.noun.1sg@"  0:"t2" > ANSTEMLIST_kw ; ! 1
< 0:k 0:i "@U.noun.2sg@"  0:"t2" > ANSTEMLIST_kw ; ! 2
< 0:o     "@U.noun.3sg@"  0:"t2" > ANSTEMLIST_kw ;     ! 3
< 0:o     "@U.noun.3isg@" 0:"t2" > ANSTEMLIST_kw ;    ! 3-singular-obviative
< 0:n 0:i "@U.noun.1pl@"  0:"t2" > ANSTEMLIST_kw ;   ! exclusive Pl
< 0:k 0:i "@U.noun.12pl@" 0:"t2" > ANSTEMLIST_kw ; ! inclusive Pl
< 0:k 0:i "@U.noun.2pl@"  0:"t2" > ANSTEMLIST_kw ;  ! 2pl 
< 0:o     "@U.noun.3pl@"  0:"t2" > ANSTEMLIST_kw ;       ! 3pl
< 0:o     "@U.noun.3ipl@" 0:"t2" > ANSTEMLIST_kw ;       ! 3-plural-obviative



